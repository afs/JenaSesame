<ivysettings>
  <!-- Resolvers for dependences -->
  <resolvers>
    <chain name="resolver-chain">

      <ibiblio name="jena-dev" root="http://openjena.org/repo-dev" m2compatible="true" useMavenMetadata="false"/>

      <!-- Jena publish repository, sync'ed to maven central
      <ibiblio name="jena" root="http://openjena.org/repo" m2compatible="true"/>
      -->

      <ibiblio name="openrdf" 
	       root="http://repo.aduna-software.org/maven2/releases" 
	       m2compatible="true"/>

      <ibiblio name="maven2" m2compatible="true" />

    </chain>
  </resolvers>

  <!-- Resolvers used for getting our dependencies from
       Defines "resolver-chain" 
  -->

  <settings defaultResolver="resolver-chain"/>
  <!--
  <caches defaultCacheDir="c:/home/afs/.ivy2/cache"/>
  -->

</ivysettings>
